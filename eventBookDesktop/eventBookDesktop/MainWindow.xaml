<Window x:Class="eventBookDesktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:eventBookDesktop"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <WrapPanel  >

        <WrapPanel.Resources>
            <Style TargetType="{x:Type Button}">
                <Setter Property="Margin" Value="10,0" />
            </Style>
        </WrapPanel.Resources>
        

        <DataGrid HorizontalAlignment="Left" VerticalAlignment="Top" x:Name="grdEvents" />

        <Button Click="InsertLineTest" Width="84" Height="28" Canvas.Left="313" Canvas.Top="187">Insert</Button>



    </WrapPanel>
</Window>
